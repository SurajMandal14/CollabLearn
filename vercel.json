{
  "version": 2,
  "framework": null,
  "outputDirectory": "dist",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev:all",
  "installCommand": "npm install",
  "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
